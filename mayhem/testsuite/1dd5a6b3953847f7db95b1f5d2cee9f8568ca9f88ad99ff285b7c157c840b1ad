	* equ $2000

	ilistdw end1-4*23000
	el
end2	dw (end1-4)*2
end1	dw $ad
	
	if 1<equ2
	  if 1>2
	    dl 3000
	  else
	dw 4000
	  endc
	  dw 1000
	else
	  if 1<>1
	    dw 6000/2
	  endc
	  dw 2000
	endc

	clist off
	
	if ofr
	  dw 3000
	else
	  dw 4000
	endc
	
	if 1<=1
	  
	clist off
	
	if ofr
	  dw 300dw 5000
	  if 1>2
   dw 3000
	  else
	dw 4000
	  endc
	endm
	
	
