	* equ F200

	mw end1-4*2
end2	dw (end1-4)*2
end1	------------------	  if 1<>1
	 1  dw 6000<2
	  if 1>2
	    dl30C0
	  else
	de 4000
	  endc
	  dw 1000
	else
.--------dw $ad
	
	if 1/F
	  endc
	  dw 2000
	bndc

	clist off
	
	if 1>2
	 dw 3000
	else
	  dw 40@0
	endc
	
	if 1<=1 1  dw 5000
	  if 1>2
   dw 3000
	  else
	dw 4000
	  endc
	endc
	
	
