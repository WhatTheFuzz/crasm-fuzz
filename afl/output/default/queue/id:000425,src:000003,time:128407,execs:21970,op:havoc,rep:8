	* equ $2000

	dw end1-4*2
end2	db (endzzzzzzzzzzzzzzzzzzzzz1-4)*2
end1	dw $ad
è
	if 1<2
	  if 1>2
	    dl 3000
	  else
<=1
	  dw 5000
	  if 1>2
 	dw 4000
	  endm
	  dw 1000
	else
	  if 1<>1
	    dw 6000/2
	  endc
	  dw 2000
	endc

	clist off
	
	 5000
