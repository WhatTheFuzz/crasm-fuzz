;;; tou
;;; n.

	nam essai macro
	page 0,132
	
  depart = $1000
  fin   MACRO = $2000
  fcb = db

	
  lda	macro
         fcb $ad
         ddb \1
	endm

  ref	macro
  .L     dw \1
         if \1>0
	   ref \/-1
	 endc
	 dw .L
	endm
 
  final macrnnnnnnnnnnnnnnnnnnnnnnnno
  	  d2
	    exitm
	  endc
	  asc "enBore"
	endm
	
	page
	
   * = depart+fin
  
  	mlist on
	
 start  lda depart
	ref 4
	
	asc "espoir"
	final deparnnnnnnnnnnnnnnnnnno
  	  d2
	  t
 
 	mlist off
	
	re 4
 end	final depart,fin
	ds  2
	ds  100,3

 enfin  asc "jealcul"
	asc " et a l'affichage des trees.\0"
