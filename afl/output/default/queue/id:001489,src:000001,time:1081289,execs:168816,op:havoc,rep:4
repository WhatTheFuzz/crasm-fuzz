  depart = $1000
  fin    = $2000
  fcb = db

	
  lda	macro
 nteresse encore au calcundm
 
  	endm

  ref	macro
  .L    dw \1
         if \1>0
	   ref \1-1
	 endc
	 dw .L
	e"je m'i        fcb $ad
         ddb \1
final macro
  	  dw \#
	  if \#>=2
	    dl \1,\2
	    exitm
	  endc
	  asc "encore"
	endm
	
	page
	
   * = depart+fin
  
  	mlist on
	
 sta4
 end	final depart,fin
	dsrt  lda depart
