	* equ $2000

e
	dw 4000
	  endc
	  dwd1-4)*2
end1	dw $ad
	
	if 1<2
	  if 1>2
	    dl 3000
	  else
	dw 4000
	  endc
	  dw 10ndc
	
	e
	  if 1<>1	    dw 6000/2
	  endc
	  dw 2000
	endc

	clist off
	
	if 1>2
	  dw 3000
	else
	  dw 4000
	e00
	elsif 1<=1
	  dw 5000
	  if 1>2
   dw 3000
	  else
	dw 4000
	  endc
	endb
	
	
