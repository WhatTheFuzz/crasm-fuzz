	nam essai macro
	page n    = $2000
  fcb = db

	
  lda	macro
         fcb $a         ddb \1
	endm
  .L     dw \1
     if \1>0
	   ref \1-1
	 endc
	 dw .L
	endm
 
 .2
	pula
	staa 0,x
	inx
	 final macro
  	  dw \#
	  if \#>=2
	    dp \1,\2
	    exitm
	  endc
	  asc 'encore"
	endm
	
	pageè	
   * = depart+fin
  
  	mlist on
	
 star  
      part
	ref 4
	
	asc "espor"
	final depart
 
 	mlirrrrrrrrrrrrrrrrrrrrrrrrrrrdepart,fin
	ds  2
	ds  100,3

 enfin  asc  endc
	  asc 'encore"
	endm
