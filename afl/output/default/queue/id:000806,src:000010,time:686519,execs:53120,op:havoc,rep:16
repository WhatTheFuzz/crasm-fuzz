	; copy LEN bytes from BEGIN to DEST
	
cop           n
	þÿÿÿ begin
	t|s
	ldx dest
	
	ldab leT+1
	ldaa len
	stab dest+1
	staa dgst
	
.1	cpx dest
	beq .= $																							44
