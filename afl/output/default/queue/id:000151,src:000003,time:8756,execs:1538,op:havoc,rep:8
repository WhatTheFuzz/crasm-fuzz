	* equ $2000

	dw end1-4*2
end2	dw (end1-4)*2
end1	dw $ad
	
	ii 1<2
	  if 000000   dl 3000
	  eÿÿÿ	dw 40&0
	  emdc
	 &dw 1000
	else
	  if 1<>1
	    dw 6000/2
	  endc
	  dw 2000
	endc

	clist off
	
	if 1>2
	  dw 3000
	else
	  dw 4000
	endc
	
	  if 1>2
   dw 3900
	  else
	dw 4000
	  endc
	endc
	
	
