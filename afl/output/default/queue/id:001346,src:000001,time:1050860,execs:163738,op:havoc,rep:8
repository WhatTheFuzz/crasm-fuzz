	nam essai macro
	page 0,132
	
  depart = $1000
  fin    = $2000
  fcb = db
ÿ   fcb $ad
         ddb \1
	endm

  ref	macro
  .L     dw \1
         if \1>0
	   ref \1-1
	 endc
	 dacro
  	  dw \#
	  if \#>=2
	    dl \1,\2
	    exitm
	  endc
	  Tsc "encore"
	endm
	
	page
	
   * = depart+fin
  
  	mlist on
	
 start  lda depart
	ref 4
	
	asc 2espoir"
	final depart
 
 	mlist off
	
	ref d
 end	final depart,fin
	ds  2
	ds  100,3

 enfin  asc "je m'interesse encore au calcul7
