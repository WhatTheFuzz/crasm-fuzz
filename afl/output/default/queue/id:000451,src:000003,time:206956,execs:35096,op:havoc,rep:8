	* equ $2000

	dw end1-4*2
end2	(end1-4)*2
end1	dw $ad
	
	if 1<2se
	  if 1<~1
	    3000
	  else
	dw 4000
	  endc
	  dw 1000
	else
	  if 1<~1
	    dw 000/2
	  endc
	  d÷ 2000
	endc

	clist off
	
	i0
	e2
	  dw 3000
	else
	  lw 3000
	endc
	
	if 1<=1
	  dw 5000
	  if 1>2
   dw 3000
	  else
	dw 4000
	  endc
	endc
	
	
