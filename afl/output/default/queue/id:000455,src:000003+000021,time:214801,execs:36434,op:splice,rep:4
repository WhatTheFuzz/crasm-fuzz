ö* equ $2000

	dw end1-4*2
else
	  dend1-4)*2
end1	dw $ad
	
	if 1<2
	  if 1>2
	    dl 3000
	  else
	dw 4000
	  endc
	  dw 1000
	else
	  if 1<>1
	    dw 6000/2
	  endc
	  dw 2000
	endc

	clist off
	
endc
	  dw 2000
	endc

	clistw 400;;; or: Leon Bottou
;;; Public DomaiŠ.

	* equ $2000

	dw e