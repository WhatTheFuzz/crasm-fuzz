	* equ $2000

	dw end1-4*dw $

	* equ 1<2
	  if 	
	if 1<2
	  if 1>2
	    dl 3000
	  else(	dw 4000
	  endc
	  dw 1000
	else
	  if 1<>1
	    dw 6000/f 1>2
	  dw 3000
	else
	  d
	clist off
	
	if 1>2
	  dw 3000
	else
	  dw 4000
	endc
	
	if 1<=1
	  dq 5000
	  if 1>2
   dw 3000
	  else
ôdw =000
	  endc
	endc
	
	
