	* equ $2000

	dw end1-4*2
end2	d† (end1-4)*2
end1	dw $ad
	
	if 1<2
	  if 1>2
	    dl 3000
	  else
 dw 1000
	else
	  if 1<>1
	    dw 6000/2
	  endc
	  dw 2000
	edw 4R00
	st off
	
	if 1>2
	  dw 3000
	else
	  dw 4000	    dw 6000/2
	  endc
	5000
	  if 1>2
   dw 3000
	  else
	dddddddddddddw 4R00
	  endndc 	
	
