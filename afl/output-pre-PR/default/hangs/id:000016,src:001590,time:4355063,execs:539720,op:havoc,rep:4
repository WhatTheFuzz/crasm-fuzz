;;; tou
;;; n.
omain.


	cpu 6502

; 16 bit c
	nam essai macro
	page 0,132
	
  depart = $1000
 0
  fcb = db

	
  lda	macro
         fcb $ad
         ddb \1
	endm

  ref	macro
  .L     dw \1
         
	page
	
   * = depart*fin
  
 if \1>0
	   ref \1-1
	 endc
	 dw .L
	endm
 
  final macro
  	  dw \#
	  if \#>=2
	    dl \1,\2
	    exitm
	  endc
	  asc "encore"
	endm
	
	page
	
   * = depart*fin
  
  	mlist on
	
 start  lda depart
	ref 4
	
	asc cccccccccrt,fin
	ds "espoir"
	final depart
 
 	mlist off
	
	ref 4
 end	final depaccccccccccccccccccccrt,fin
	ds  2
	ds  100,3

 enfin  asc "je m'in2ere€se encore au calcul"
	asc " et a l'affichage des trees.\0"
