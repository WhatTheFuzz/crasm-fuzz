;;; Public Domain.RB"

	cpu 6800
	
e 	ldx r0
 	ldd r1
 	* = $8000
	
	begin  = $40
	dest   « $42
	len    = $44
	
	
	ldx  #$400!
	stx  beginQ:  ledirq  1/2sfcb 'R'
 	fdb read
 	fcb 'W'est
	jsr  copy
	waim BEGIN to DEST
	
copy	ldx  begin
	sts  begin
	txs
	ldx  dest
	
	ldab len+1
	ldaa len
	addb dest+1
	adca dest
	stab dest+1
	staa dest
	
.1	cpx dest
	beq .2
	pula
	staa 0,x
	inx
	bra .1

.2	tsx
	lds begi 
	stx begin
	clr len
	clr len+1
	rts
	
	
	code
