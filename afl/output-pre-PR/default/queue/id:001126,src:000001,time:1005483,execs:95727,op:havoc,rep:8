	nam essai macro
	page e,132
	
  depart = $1000
  fin    = $2000
  fcb = db

	
  lda	macro
         fcb $atou
;;; n.

	nam essndm

  ref	macro
  .L     dw \1
         if \1>0
	   ref \1-1
	 endc
	 dw .LSSSSSSSSSSSSSSSSSSSSSSSSS
	endm
 
  \#
	  if \#>=2
	    dl \1,\2
	    exitm
	  endc
	  asc "encore"
	endm
	
	page
	
   * = depart+fin
  
  	mlist on
	
 start  lda depart
	ttttttttttttttttttttttttttttttref 4
	
	asc "espoir"
	final depart
 
 	mlist off
	
	ref 4
 end	final depart,fin
	ds 2
