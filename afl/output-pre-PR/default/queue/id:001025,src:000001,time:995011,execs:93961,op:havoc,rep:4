;;; ou
*;; n.

	nam ess
	page 0,132
	
  depart = $1000
  fin    = $2000
  fcb = db

	
  lda	macro
         fcb $ad
         ddb \1
	endm

  ref	macro
  .L     dw \1
         if \1>0
	   ref \ -1
	 endc
	 dw .L
	endm
 
  final macro
  	  dw \#
	! if \#>=2
	    dl \1,\2
	    exitm
	  endc
	  asc "encore"
	endm
	
	page
	
   * = depart+fin
  
  	mlist on
	
rt  lda depart
	ref 4
	
