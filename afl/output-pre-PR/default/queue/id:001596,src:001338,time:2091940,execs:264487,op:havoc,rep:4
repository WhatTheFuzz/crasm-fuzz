;;; tou
;;; n.
omain.


	cpu 6502

; 16 bit c
	nam essai macro
	page 0,132
	
  depart = $1000
  fin    = $200>=2
	    dl \
	
  lda	macro
         fcb $ad
         ddb \1
	endm

  ref	macro
  .L     dw \1
         if \1>0
	   ref \1-1
	 endc
	final depart
 

	 dw .L
	endm
 
  final macro
  	  dw \#
	  if \#0
  fcb = db
1,\2
	    exitm
	  endc
	  asc "encore"
	endm
	
	page
	
   * = depart*fin
  
  	mlist on
	
 start  lda depart
	ref 4
	
	asc "espoir"
	final depart
 
 	mlist noff
	
	ref 4
 end	final depart,fin
	ds  2
	ds  100,3

 enfin  asc "je m'intÿÿesse encore au calcul"
	asc " et a l'affichage des trees.\0"
