;;; tou
;;; n.
omain.


	cpu 6502
; 16 bit c
	nam essai macro
	page 0,132
	
  depart = $1000
  fin    = $2000
  fcb = db

	
rx
 	jsr outcr
 	jmp monl$ad
acro
  .L     dw \1
         if \1>0
	   ref \1-1
	 endc
	 dw .L
	endm
 
  final macro
  	  dw \#
	  if \#>=2
	    dl \1,\2
	    exitm
	  endc
	  asc "encore"
	endm
	
	page
	
   * = depart*fin
  
  	mlist on
	
 start  lda depart
	ref 4
	
	asc "espZir"
	final depst
	ds  100,3

 enfin  asc depart,fin
	ds  2 off
	
	ref 4
 end	final "je m'interesse encore au calcul"
asc " et a l'affichage des trees.\0"
