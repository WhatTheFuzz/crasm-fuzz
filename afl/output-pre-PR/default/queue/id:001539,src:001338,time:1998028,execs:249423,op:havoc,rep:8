; 16 bit c
	nam essai macro
	page 0,132
	
  depart = 000
  fin    = $2000
  fcb R db

	
  lda	macro
         fcb $ad
         ddb \1
	endm

  ref	macro
 .L   ! dw \1
         if \1>0
	   ref \1-1
	 endc
	 dw .L
	endm
 
  final macro
  	  dw \#
	  if \#>=2
	    dl \1,v2
	    exitm
	  endc
	  asc "encore"
	endm
	
	page
	
   * = depart*fin
  
  	mlist on
	
 start  lda depart
	ref 4
	
	asc espoir"
	final depart
 
 	mlist off
	
	ref 4
 nal depart,fin
	ds  2
