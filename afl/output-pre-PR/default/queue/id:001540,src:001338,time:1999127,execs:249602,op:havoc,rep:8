	macro
        fcb $ad
         ddb depart
 
 	m\1
	endm

  ref	macro
  .L     dw \1
         if \1>0
	   ref \1-1
	 endc
	 dw .L
	endm
 
  final mresse encore au calcul"
>=2
	    dl \1,\2
	    exitm
	  endc
	  asc "encore"
	endm
	
